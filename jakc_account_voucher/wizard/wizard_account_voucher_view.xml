<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="wizard_account_voucher_report" model="ir.ui.view">
			<field name="name">wizard.account.voucher.report</field>
			<field name="model">account.voucher.report</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Report Options">
					<group col="2" colspan="2">
						<field name="date_from" />
						<field name="date_to" />
						<field name="res_user_id" />
                        <newline />
                    </group>
					<footer>
						<button string="Cancel" special="cancel" />
                        <button name="print_report" string="Print" type="object" class="oe_highlight" />
					</footer>
				</form>
			</field>
		</record>

        <record id="action_wizard_account_voucher_report" model="ir.actions.act_window">
            <field name="name">Account Voucher Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">account.voucher.report</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
        </record>

        <menuitem action="action_wizard_account_voucher_report"
                  id="menu_action_wizard_sale_report_inventory"
                  sequence="206" parent="account.menu_finance_legal_statement"/>

			


	</data>
</openerp>
