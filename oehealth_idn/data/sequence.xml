<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Unit Registration -->
        <record id="seq_unit_registration" model="ir.sequence">
            <field name="name">Unit Registration</field>
            <field name="code">unit.registration</field>
            <field name="prefix">REG-</field>
            <field name="padding">6</field>
        </record>

        <!-- Replace Sequence Patient -->
        <record id="oehealth.seq_oeh_medical_patient" model="ir.sequence">
            <field name="name">Patient</field>
            <field name="code">oeh.medical.patient</field>
            <field name="prefix"></field>
            <field name="padding">9</field>
        </record>

        <!-- Sequence Patient Dummy -->
        <record id="seq_oeh_medical_patient_dummy" model="ir.sequence">
            <field name="name">Patient Dummy</field>
            <field name="code">oeh.medical.patient.dummy</field>
            <field name="prefix">D</field>
            <field name="padding">8</field>
        </record>

        <!-- Replace Sequence Walkin to Arrival -->
        <record id="oehealth_extra_addons.seq_oeh_medical_appointment_register_walkin" model="ir.sequence">
            <field name="name">Arrival</field>
            <field name="code">oeh.medical.appointment.register.walkin</field>
            <field name="prefix">AR-%(y)s%(month)s%(day)s</field>
            <field name="padding">3</field>
        </record>

        <!-- Dummy Arrival Sequence -->
        <record id="seq_dummy_arrival" model="ir.sequence">
            <field name="name">Dummy Arrival</field>
            <field name="code">dummy_arrival</field>
            <field name="prefix">D-%(y)s%(month)s%(day)s</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>