<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_wizard_physician_income_report_form" model="ir.ui.view">
            <field name="name">wizard.physician.income.report.form</field>
            <field name="model">wizard.physician.income.report</field>
            <field name="arch" type="xml">
                <form string="Physician Income Report">
                    <group>
                        <field name="start_date"/>
                        <field name="end_date"/>
                    </group>
                    <group>
                        <field name="physician_ids" colspan="4" nolabel="1" />
                    </group>
                    <footer>
                        <button name="generate_report" string="Print" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_report_physician_income" model="ir.actions.act_window">
            <field name="name">Physician Income Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">wizard.physician.income.report</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        
       <menuitem  id="menu_physician_income_report" action="action_report_physician_income"
       	sequence="206" parent="account.menu_finance_legal_statement" />

</odoo>