<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Form view of Partner -->
    <record model="ir.ui.view" id="view_users_form_inherit">
        <field name="name">view_users_form_inherit</field>
        <field name="model">res.users</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="operating_unit.view_users_form"/>
        <field name="arch" type="xml">
            <field name="default_operating_unit_id" position="after">
                <field name="default_unit_administration_id" domain="[('operating_id','=',default_operating_unit_id)]"/>
            </field>
        </field>
    </record>

</odoo>